{
  "name": "movies-look-up",
  "version": "0.0.1",
  "description": "simple project to get and display info from moviedb",
  "main": "index.js",
  "scripts": {
    "versions:check": "ncu -x eslint-plugin-jsx-a11y",
    "versions:update": "ncu -u -x eslint-plugin-jsx-a11y",
    "report:dep:issues": "node scripts/depcheck",
    "report:dep:usage": "node scripts/depcheck usage",
    "report:metrics": "rm -rf report/plato && plato -r -d report/plato --eslint .eslintrc.json src",
    "report:bundles": "rm -rf build/public/bundles && mkdir -p report/bundles/ && webpack --config webpack.report.config.js",
    "report": "npm run report:dep:issues && npm run report:dep:usage && npm run report:metrics && npm run report:bundles",
    "start:pm2": "pm2 start ecosystem.json",
    "stop:pm2": "pm2 stop ecosystem.json && pm2 kill",
    "start:node": "node build/node/server.js",
    "stop:node": "pkill -f 'movies-look-up-node'",
    "lint:js": "eslint './**/*.js' './**/*.jsx' --config '.eslintrc.json' --ignore-path '.eslintignore'",
    "lint:js:fix": "eslint './**/*.js' './**/*.jsx' --config '.eslintrc.json' --ignore-path '.eslintignore' --fix",
    "lint:json:test": "jsonlint-cli ./src/**/*.json",
    "lint:json": "jsonlint-cli .babelrc && jsonlint-cli ./*.json && jsonlint-cli ./src/**/*.json && jsonlint-cli ./test/**/*.json && jsonlint-cli ./scripts/**/*.json",
    "lint": "npm run lint:json && npm run lint:js",
    "test:node": "mocha --compilers js:babel-core/register 'src/**/*.spec.*'",
    "test": "npm run test:node",
    "compile:html": "node scripts/generate-static-html -s './src/public/**/*.html.js' -o './build/public/'",
    "compile:assets": "rm -rf build/public/assets && mkdir -p build/public/assets && cp -a src/public/assets/. build/public/assets/",
    "compile:node": "babel src/node --out-dir build/node --source-maps inline --ignore spec.js,spec.data.js",
    "compile:universal": "babel src/universal --out-dir build/universal --source-maps inline --ignore spec.js,spec.data.js",
    "compile:public": "webpack",
    "compile": "npm run compile:node && npm run compile:universal && npm run compile:public && npm run compile:html && npm run compile:assets",
    "build": "npm run lint && npm run compile",
    "watch:html": "watch -c 'npm run compile:html' -p './src/public/**/*.html.js' & watch -c 'npm run compile:html' -p './src/public/**/*.marko'",
    "watch:assets": "watch -c 'npm run compile:assets' -p './src/public/assets/**/*'",
    "watch:public": "webpack --watch & watch -c 'npm run compile:html' -p './build/public/bundles/**/*.*'",
    "watch:universal": "watch -c 'npm run compile:universal' -p './src/universal/**/*.js' & nodemon -w ./build/node ./build/node/server.js",
    "watch:node": "watch -c 'npm run compile:node' -p './src/node/**/*.js' & nodemon -w ./build/node ./build/node/server.js",
    "watch": "npm run watch:public & npm run watch:html & npm run watch:universal & npm run watch:node & npm run watch:assets",
    "release": "npm run build && node scripts/release.js -m 'patch'",
    "prepush": "npm run lint && npm run test:node"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "engineStrict": true,
  "engines": {
    "node": "7.10.0",
    "npm": "4.2.0"
  },
  "author": "Nathan Richards <nathan.richards.500@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0",
    "pm2": "^2.1.5"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.7.4",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-react": "^6.11.1",
    "bootstrap-sass": "^3.3.6",
    "chai": "^3.5.0",
    "css-loader": "^0.28.1",
    "cssnano": "^3.8.0",
    "currency-symbol-map": "^3.1.0",
    "depcheck": "^0.6.4",
    "enzyme": "^2.7.1",
    "es5-shim": "^4.5.9",
    "es6-shim": "^0.35.1",
    "eslint": "^3.10.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "firebase": "^4.0.0",
    "glob": "^7.1.1",
    "global-npm": "^0.3.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.15.0",
    "husky": "^0.11.9",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^9.11.0",
    "json-loader": "^0.5.4",
    "jsonlint-cli": "^1.0.1",
    "marko": "^3.12.1",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.2",
    "mockery": "^2.0.0",
    "mockfirebase": "^0.12.0",
    "nock": "^9.0.9",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "npm-check-updates": "^2.8.0",
    "plato": "^1.7.0",
    "postcss-cli": "^2.5.1",
    "postcss-loader": "^2.0.5",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.5",
    "sinon": "^1.17.7",
    "style-loader": "^0.17.0",
    "supertest": "^3.0.0",
    "watch-cli": "^0.2.1",
    "webpack": "^2.5.1",
    "webpack-cleanup-plugin": "^0.4.1",
    "webpack-manifest-plugin": "^1.0.1",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
